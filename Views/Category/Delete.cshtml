@model Category;

<form method="post">
	<input asp-for"Id" hidden />
	<div class="border p-3 mt-3 border-black">
		<div class="row pb-2">
			<h1 class="text-dark">Delete Category</h1>
		</div>

		<div class="mb-2">
			<label class="text-dark">Category Name</label>
			<input asp-for="Name" class="form-control" disabled />
		
		</div>

		<div class="mb-2">
			<label class="text-dark ">DisplayOrder</label>
			<input asp-for="DisplayOrder" class="form-control" disabled/>
			
		</div>

		<div class="row">
			<dev class="col-6">
				<button type="submit" class="btn btn-danger" style="width:150px">Delete</button>
			</dev>

			<dev class="col-6">
				<a asp-controller="Category" asp-action="Index" class="btn btn-outline-dark" style="width:150px">Back</a>
			</dev>
		</div>
	</div>
</form>

